{
  "name": "@blackjack-deck/icons",
  "version": "0.1.0",
  "description": "React icon components for playing card suits (spades, hearts, diamonds, clubs) with outline and filled variants",
  "keywords": ["blackjack", "cards", "icons", "react", "svg", "suits"],
  "author": "Fredrik Johansson",
  "repository": {
    "type": "git",
    "url": "https://github.com/fjohanssondev/blackjack-deck"
  },
  "files": ["dist"],
  "license": "MIT",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./outline": {
      "types": "./dist/outline/index.d.ts",
      "import": "./dist/outline/index.js",
      "require": "./dist/outline/index.cjs"
    },
    "./filled": {
      "types": "./dist/filled/index.d.ts",
      "import": "./dist/filled/index.js",
      "require": "./dist/filled/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "react": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}